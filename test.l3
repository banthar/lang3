
function printf(format:*Byte,...):Int;
function atoi(a:*Byte):Int;

struct Point = struct {
    x:Int;
    y:Int;
}

function sqrt(n:Int):Int {

	var k:Int=n;

	while(true) {
		var k1:Int=(k+n div k) div 2;

		if(k1==k)
			return k;

		k=k1;

	}

	return k;

}

function length(point:Point):Int {
	return sqrt(point.x*point.x+point.y*point.y);
}

