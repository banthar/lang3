
function printf(format:*Char,...):Int;
function atoi(a:*Char):Int;

type Point = struct
{
    x:Int;
    y:Int;
}

function sqrt(n:Int):Int
{

	var k:Int=n;

	while(true)
	{
		var k1:Int=(k+n div k) div 2;

		if(k1==k)
			return k;

		k=k1;

	}

	return k;

}

function length(point:Point):Int
{
	return sqrt(point.x*point.x+point.y*point.y);
}

function main(argc:Int,argv:**Char):Int
{

	if(argc!=3)
	{
		printf("wrong number of arguments\n",10);
		return 1;
	}

	var point:Point;

	point.x=atoi(argv[1]);
	point.y=atoi(argv[2]);

    printf("length((%i, %i)) = %i\n",point.x, point.y, length(point));
    return 0;
}


